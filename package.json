{
  "name": "y",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npx webpack serve --env port=3001 --env mode=development",
    "start:dev-server": "node ./json-server/index.js",
    "build:prod": "npx webpack  --env mode=production",
    "build:dev": "npx webpack  --env mode=development",
    "lint:ts": "./node_modules/eslint/bin/eslint.js '**/*.{ts,tsx}'",
    "lint:ts:fix": "./node_modules/eslint/bin/eslint.js --fix '**/*.{ts,tsx}'",
    "lint:scss": "./node_modules/stylelint/bin/stylelint.mjs '**/*.scss'",
    "lint:scss:fix": "./node_modules/stylelint/bin/stylelint.mjs '**/*.scss' --fix",
    "test:unit": "jest --config=./config/jest/jest.config.ts",
    "test:ui": "./node_modules/loki/bin/loki test --host $(ipconfig getifaddr en0) --requireReference",
    "test:ui:update": "./node_modules/loki/bin/loki update --host $(ipconfig getifaddr en0)",
    "test:ui:ok": "./node_modules/loki/bin/loki approve --host $(ipconfig getifaddr en0)",
    "test:ui:ci": "./node_modules/loki/bin/loki --requireReference --reactUri file:./storybook-static",
    "test:ui:report": "npm run test:ui:generate_json && npm run test:ui:generate_html",
    "test:ui:generate_json": "rm ./.loki/report.json & node scripts/generate-visual-json-report.js",
    "test:ui:generate_html": "rm ./.loki/report.html & ./node_modules/reg-cli/dist/cli.js --from .loki/report.json --report .loki/report.html",
    "storybook": "./node_modules/storybook/index.js dev -p 6006 -c config/storybook",
    "storybook:build": "./node_modules/storybook/index.js build -c config/storybook",
    "prepare": "husky install"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.5.3",
    "@storybook/react": "^7.5.3",
    "@storybook/react-webpack5": "^7.5.3",
    "@storybook/testing-library": "^0.2.2",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.8.6",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@types/react-router-dom": "^5.3.3",
    "@types/webpack": "^5.28.3",
    "@types/webpack-bundle-analyzer": "^4.6.3",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "eslint": "^8.54.0",
    "eslint-config-standard-with-typescript": "^40.0.0",
    "eslint-plugin-i18next": "^6.0.3",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.15",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "json-server": "^0.17.4",
    "loki": "^0.33.1",
    "mini-css-extract-plugin": "^2.7.6",
    "reg-cli": "^0.17.7",
    "sass": "^1.69.3",
    "sass-loader": "^13.3.2",
    "storybook": "^7.5.3",
    "style-loader": "^3.3.3",
    "stylelint": "^15.11.0",
    "stylelint-config-standard-scss": "^11.1.0",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.7",
    "axios": "^1.6.2",
    "i18next": "^23.7.6",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-http-backend": "^2.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.5.0",
    "react-redux": "^9.0.0-rc.0",
    "react-router-dom": "^6.16.0"
  },
  "loki": {
    "pixelmatch": {
      "threshold": 0.2
    },
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.docker",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false
      },
      "chrome.iphone7": {
        "target": "chrome.docker",
        "preset": "iPhone 7"
      }
    }
  }
}
